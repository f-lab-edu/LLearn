{
  "dashboard": {
    "id": null,
    "title": "Curriculum Platform Dashboard",
    "tags": ["curriculum", "education"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "LLM 성공률",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(llm_requests_total{status=\"success\"}[5m]) / rate(llm_requests_total[5m]) * 100",
            "legendFormat": "성공률 %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API 응답시간",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "비즈니스 메트릭",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_registrations_total[1h])",
            "legendFormat": "사용자 등록/시간"
          },
          {
            "expr": "rate(curriculums_created_total[1h])",
            "legendFormat": "커리큘럼 생성/시간"
          },
          {
            "expr": "rate(summaries_created_total[1h])",
            "legendFormat": "요약 생성/시간"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "현재 통계",
        "type": "table",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "총 사용자 수"
          },
          {
            "expr": "curriculums_total",
            "legendFormat": "총 커리큘럼 수"
          },
          {
            "expr": "summaries_total",
            "legendFormat": "총 요약 수"
          },
          {
            "expr": "feedbacks_total",
            "legendFormat": "총 피드백 수"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "소셜 활동",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(likes_total[1h])",
            "legendFormat": "좋아요/시간"
          },
          {
            "expr": "rate(bookmarks_total[1h])",
            "legendFormat": "북마크/시간"
          },
          {
            "expr": "rate(comments_total[1h])",
            "legendFormat": "댓글/시간"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
